{
    "name": "Descargar archivos de Asana",
    "version": "1.0",
    "description": "Extensi√≥n para descargar varios archivos a la vez desde Asana",
    "manifest_version": 2,
    "permissions": [
        "downloads",
        "activeTab"
    ],
    "icons": {
        "16": "icon-16.png",
